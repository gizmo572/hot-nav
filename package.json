{
  "name": "hot-nav",
  "version": "1.0.0",
  "description": "intuitive accessibility hot keys",
  "type": "module",
  "exports": {
    "import": "./dist/esm/index.jsx",
    "require": "./dist/cjs/index.jsx"
  },
  "main": "dist/cjs/index.jsx",
  "module": "dist/esm/index.jsx",
  "files": [
    "dist/cjs",
    "dist/esm"
  ],
  "scripts": {
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "author": "Nicholas Cushman",
  "license": "ISC",
  "devDependencies": {
    "@types/chroma-js": "^2.4.4",
    "@types/react": "^18.2.67",
    "next": "^14.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.2"
  },
  "dependencies": {
    "chroma-js": "^2.4.2"
  }
}
